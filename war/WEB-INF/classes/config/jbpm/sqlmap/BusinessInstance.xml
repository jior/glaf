<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="BusinessInstance">

  <typeAlias alias="businessInstance" type="org.jpage.jbpm.model.BusinessInstance"/>

  <update id="BusinessInstance@updateBusinessStatus" parameterClass="java.util.Map">
           UPDATE JPAGE_JBPM_BUSINESS_INSTANCE 
		   SET businessStatus = #businessStatus#,
		       wfStatus = #wfStatus#
		   WHERE ( processInstanceId = #processInstanceId# )
  </update>

</sqlMap>