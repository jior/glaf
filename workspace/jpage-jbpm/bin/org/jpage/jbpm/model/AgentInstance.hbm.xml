<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="org.jpage.jbpm.model.AgentInstance" table="JPAGE_JBPM_AGENT_INSTANCE" >
  <cache usage="read-write"/>
  <id name="id" column="id" type="string" length="32" unsaved-value="null">
   <generator class="assigned"/>
  </id>
  <property name="actorId" column="actorId" type="string" length="100" not-null="true"/>
  <property name="agentId" column="agentId" type="string" length="100" not-null="true"/>
  <property name="objectId" column="objectId" type="string" length="255"/>
  <property name="objectValue" column="objectValue" type="string" length="255"/>
  <property name="attribute01" column="attribute01" type="string" length="255"/>
  <property name="attribute02" column="attribute02" type="string" length="255"/>
  <property name="startDate" column="startDate" type="timestamp"/>
  <property name="endDate" column="endDate" type="timestamp"/>
 </class>

</hibernate-mapping>
