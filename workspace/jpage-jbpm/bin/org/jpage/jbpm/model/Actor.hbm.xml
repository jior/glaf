<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="org.jpage.actor.Actor" table="JPAGE_JBPM_ACTOR" >
  <cache usage="read-write"/>
  <id name="id" column="id" type="string" length="32" unsaved-value="null">
   <generator class="assigned"/>
  </id>
  <property name="name" column="name" type="string" length="50" not-null="true"/>
  <property name="actorId" column="actorId" type="string" length="100" not-null="true"/>
  <property name="roleId" column="roleId" type="string" length="255" not-null="true"/>
  <property name="objectId" column="objectId" type="string" length="255"/>
  <property name="objectValue" column="objectValue" type="string" length="255"/>
 </class>

</hibernate-mapping>
