<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="org.jpage.jbpm.model.MessageInstance" 
        table="JPAGE_MESSAGE_INSTANCE" >
  <id name="messageId" column="MESSAGEID_" type="string" length="100" unsaved-value="null">
   <generator class="assigned"/>
  </id>
  <property name="messageTo" column="MESSAGETO_" type="string" length="255"/>
  <property name="messageType" column="MESSAGETYPE_" type="string" length="50"/>
  <property name="senderId" column="SENDERID_" type="string" length="255"/>
  <property name="senderName" column="SENDERNAME_" type="string" length="50"/>
  <property name="receiverId" column="RECEIVERID_" type="string" length="255"/>
  <property name="receiverType" column="RECEIVERTYPE_" type="int"/>
  <property name="objectId" column="OBJECTID_" type="string" length="255"/>
  <property name="objectName" column="OBJECTNAME_" type="string" length="255"/>
  <property name="objectValue" column="OBJECTVALUE_" type="string" length="255"/>
  <property name="taskName" column="TASKNAME_" type="string" length="50"/>
  <property name="taskDescription" column="TASKDESCRIPTION_" type="string" length="100"/>
  <property name="taskInstanceId" column="TASKINSTANCEID_" type="string" length="10"/>
  <property name="processInstanceId" column="PROCESSINSTANCEID_" type="string" length="10"/>
  <property name="processName" column="PROCESSNAME_" type="string" length="255"/>
  <property name="rowId" column="ROWID_" type="string" length="255"/>
  <property name="dataFile" column="DATAFILE_" type="string" length="255"/>
  <property name="deviceId" column="DEVICEID_" type="string" length="50"/>
  <property name="title" column="TITLE_" type="string" length="255"/>
  <property name="content" column="CONTENT_" type="string" length="255"/>
  <property name="link" column="LINK_" type="string" length="255"/>
  <property name="lastViewIP" column="LASTVIEWIP_" type="string" length="255"/>
  <property name="lastViewDate" column="LASTVIEWDATE_"  type="timestamp"/>
  <property name="createDate" column="CREATEDATE_" type="timestamp"/>
  <property name="jobDate" column="JOBDATE_" type="timestamp"/>
  <property name="status" column="STATUS_" type="int"/>
  <property name="startDelay" column="STARTDELAY_" type="long"/>
  <property name="repeatCount" column="REPEATCOUNT_" type="int"/>
  <property name="repeatInterval" column="REPEATINTERVAL_" type="long"/>
  <property name="deleteFlag" column="DELETEFLAG_" type="int"/>
  <property name="versionNo" column="VERSIONNO_" type="long"/>
 </class>
</hibernate-mapping>