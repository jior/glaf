<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.jpage.jbpm.model.LongIdInstance" table="JPAGE_JBPM_ID">
	  <id name="objectId" column="objectId" type="string" length="255" unsaved-value="null">
         <generator class="assigned"/>
      </id>
	  <property name="title" column="title" type="string" length="255"/>
	  <property name="objectName" column="objectName" type="string" length="255"/>
	  <property name="nextId" column="nextId" type="long"/>
	</class>
</hibernate-mapping>