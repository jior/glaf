<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.jpage.jbpm.model.DeployInstance" table="JPAGE_JBPM_DEPLOY">
	  <id name="objectId" column="objectId" type="string" length="32" unsaved-value="null">
         <generator class="assigned"/>
      </id>
	  <property name="title" column="title" type="string" length="100"/>
	  <property name="objectName" column="objectName" type="string" length="100"/>
	  <property name="processName" column="processName" type="string" length="100"/>
	  <property name="processDefinitionId" column="processDefinitionId" type="string" length="10"/>
	  <property name="deployDate" column="deployDate" type="timestamp"/>
	  <property name="lastModifiedTimeMillis" column="lastModifiedTimeMillis" type="long"/>
	  <property name="versionNo" column="versionNo" type="long"/>
	</class>
</hibernate-mapping>