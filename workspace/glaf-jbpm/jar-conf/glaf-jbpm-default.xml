<?xml version="1.0" encoding="UTF-8"?>

<configuration>

    <property>
		<name>jbpm.hibernate.jdbc.sync</name>
		<value>true</value>
		<description>自动同步Hibernate配置文件</description>
	</property>

	<property>
		<name>jbpm.processImageUrl</name>
		<value>/mx/jbpm/image</value>
	</property>

	<property>
		<name>RunningTasks_subject</name>
		<value>${res_system_name} ${now}待办任务提醒</value>
	</property>

	<property>
		<name>RunningTasks_template</name>
		<value>/conf/templates/mail/simpletasklist.html</value>
	</property>
 
</configuration>
