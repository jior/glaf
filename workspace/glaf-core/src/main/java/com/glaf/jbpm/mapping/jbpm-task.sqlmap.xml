				</isNotNull>
			</isPropertyAvailable>
			
			<isPropertyAvailable prepend="AND" property="actorIds">
				<isNotNull prepend="" property="actorIds">
					<iterate prepend="" property="actorIds" open="(" close=")"
						conjunction="OR">
						( a.ACTORID_ = #actorIds[]# ) 
					</iterate>
				</isNotNull>
			</isPropertyAvailable>
			
			<isPropertyAvailable prepend="AND" property="processInstanceId">
