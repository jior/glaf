  <process id="ApplicationProcess" name="采购申请单审批流程" isExecutable="true">
    <extensionElements>
      <activiti:executionListener event="start" class="com.glaf.activiti.executionlistener.RoleUsersMultiInstanceListener">
        <activiti:field name="roleId">
          <activiti:string>KZ</activiti:string>
        </activiti:field>
        <activiti:field name="outputVar">
          <activiti:string>assigneeList</activiti:string>
        </activiti:field>
        <activiti:field name="userIds">
          <activiti:string>leader01,leader02</activiti:string>
        </activiti:field>
      </activiti:executionListener>
    </extensionElements>
    <startEvent id="startevent1" name="启动流程"></startEvent>
    <userTask id="usertask1" name="科长审核" activiti:assignee="${assignee}" activiti:dueDate="${dateVariable}">
      <documentation>科长审核，包含中方与日方科长</documentation>
      <extensionElements>
        <activiti:taskListener event="complete" class="com.glaf.activiti.tasklistener.RoleUsersMultiInstanceTaskListener">
          <activiti:field name="roleId">
            <activiti:string>BZ</activiti:string>
          </activiti:field>
          <activiti:field name="outputVar">
            <activiti:string>assigneeList2</activiti:string>
          </activiti:field>
          <activiti:field name="userIds">
            <activiti:string>leader03,leader04</activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="assigneeList" activiti:elementVariable="assignee">
        <completionCondition>${!approve}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <exclusiveGateway id="exclusivegateway1" name="科长审核通过"></exclusiveGateway>
    <userTask id="usertask2" name="部长审批" activiti:assignee="${assignee}">
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="assigneeList2" activiti:elementVariable="assignee">
        <completionCondition>${!approve}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <exclusiveGateway id="exclusivegateway2" name="部长审批通过"></exclusiveGateway>
    <userTask id="usertask3" name="收单确认" activiti:assignee="dd">
      <extensionElements>
        <activiti:taskListener event="create" class="com.glaf.activiti.tasklistener.RoleUsersTaskCreateListener">
          <activiti:field name="roleId">
            <activiti:string>PO</activiti:string>
          </activiti:field>
          <activiti:field name="userIds">
            <activiti:string>leader05,leader06</activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask4" name="重新修改" activiti:assignee="joy"></userTask>
    <endEvent id="endevent1" name="完成"></endEvent>
    <exclusiveGateway id="exclusivegateway3" name="收单确认通过"></exclusiveGateway>
    <sequenceFlow id="flow1" name="提交审核" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow2" name="判断" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow3" name="退回" sourceRef="exclusivegateway1" targetRef="usertask4"></sequenceFlow>
    <sequenceFlow id="flow4" name="通过" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approve}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" name="判断" sourceRef="usertask2" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow6" name="退回" sourceRef="exclusivegateway2" targetRef="usertask4"></sequenceFlow>
    <sequenceFlow id="flow7" name="通过" sourceRef="exclusivegateway2" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approve}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" name="重新提交" sourceRef="usertask4" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow10" name="判断" sourceRef="usertask3" targetRef="exclusivegateway3"></sequenceFlow>
    <sequenceFlow id="flow11" name="退回" sourceRef="exclusivegateway3" targetRef="usertask4"></sequenceFlow>
    <sequenceFlow id="flow12" name="通过" sourceRef="exclusivegateway3" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approve}]]></conditionExpression>
    </sequenceFlow>
  </process>
