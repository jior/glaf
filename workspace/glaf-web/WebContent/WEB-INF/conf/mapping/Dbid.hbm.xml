<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="com.glaf.core.id">
    <class name="Dbid" 
           table="sys_dbid"
           dynamic-update="true"
           dynamic-insert="true">
        <id name="name" type="string" unsaved-value="null">
            <column name="NAME_" not-null="true"/>
            <generator class="assigned"/>
        </id>
      
        <property name="value" type="string" column="VALUE_"/>          
        <property name="version" type="integer" column="VERSION_"/>
		
    </class>
</hibernate-mapping>